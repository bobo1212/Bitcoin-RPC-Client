{
    "name": "bobo1212/bitcoin",
    "authors": [
        {
            "name": "Bobo1212"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bobo1212/NetCurl"
        },
        {
            "type": "composer",
            "url": "http://packagist.org"
        }
    ],
    "autoload": {
        "psr-0": {
            "Bitcoin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bitcoin\\Tests\\": "tests/"
        }
    },
    "require": {
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "config": {
        "disable-tls": true,
        "secure-http": false
    }
}
